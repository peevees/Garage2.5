@model Garage2._5.Models.ParkedVehicles

@{
    ViewBag.Title = "Receipt";
}
<div class="container tablebackground">
    <h2>Receipt</h2>


    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-horizontal tablebackground">
            <dl class="dl-horizontal">

                <dt>
                    Name:
                </dt>

                <dd>
                    @Html.ViewBag.name
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.RegistrationNumber):
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.RegistrationNumber)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.CheckIn):
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.CheckIn)
                </dd>

                <dt>
                    CheckOut:
                </dt>

                <dd>
                    @Html.ViewBag.checkOut
                </dd>
                <dt>
                    Total Time:
                </dt>

                <dd>
                    @Html.ViewBag.totalTime
                </dd>
                <dt>
                    Price:
                </dt>

                <dd>
                    @Html.ViewBag.price SEK
                </dd>
            </dl>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Brief" class="btn btn-success" />
            </div>
        </div>
        <p></p>
    }

</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

